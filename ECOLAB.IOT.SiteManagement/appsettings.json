    "AbsoluteExpirationSeconds": 300
  },
  "Health": {
    "OnlineDays": "1" //以现在的时间为准，往前倒推一个固定的时间（单位为天），在这个时间段里只要收到数据就认为在线，否则离线。
  },
  "AllowedHosts": "*"
